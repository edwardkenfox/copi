{
  "manifest_version": 2,
  "name": "copi",
  "description": "",
  "version": "1.0",
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "copi"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": false
  },
  "permissions": [
    "activeTab",
    "clipboardRead",
    "tabs"
  ]
}
